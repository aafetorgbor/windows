pipeline {
    
  agent {
             label "windows-agent"
          }

    stages{
        stage('checkout') {
            checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/aafetorgbor/windows.git']])
          
            steps {
                bat '''
                python -V

                ipconfig /all
           
                '''
            }
        }
        
        stage('BUILD') {
            steps {
                echo 'Building Docker Image'
            }
        }
        
        stage('DEPLOY') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
